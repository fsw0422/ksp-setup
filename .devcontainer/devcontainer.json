{
  "name": "ksp-setup",
  "build": {
    "context": "..",
    "dockerfile": "../Dockerfile"
  },
  "shutdownAction": "stopContainer",
  "runArgs": [
    "--rm"
  ],
  "remoteUser": "ubuntu",
  "workspaceMount": "source=${localWorkspaceFolder},target=/home/ubuntu/ksp-setup,type=bind",
  "workspaceFolder": "/home/ubuntu/ksp-setup"
}
